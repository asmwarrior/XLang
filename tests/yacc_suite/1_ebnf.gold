(grammar (decl_brace type symbol_value (symbols program statement expression)) (rules (rule root (rule_rhs (alt program (action_blk " tree_context()->root() = $1; YYACCEPT; ")) (alt error (action_blk " yyclearin; /* yyerrok; YYABORT; */ ")))) (rule program (rule_rhs (alt statement (action_blk " $$ = $1; ")) (alt (terms program ',' statement) (action_blk " $$ = MAKE_SYMBOL(\',\', 2, $1, $3); ")))) (rule statement (rule_rhs (alt expression (action_blk " $$ = $1; ")) (alt (terms ID_IDENT '=' expression) (action_blk " $$ = MAKE_SYMBOL(\'=\', 2, MAKE_TERM(ID_IDENT, $1), $3); ")))) (rule expression (rule_rhs (alt ID_INT (action_blk " $$ = MAKE_TERM(ID_INT, $1); ")) (alt ID_FLOAT (action_blk " $$ = MAKE_TERM(ID_FLOAT, $1); ")) (alt ID_IDENT (action_blk " $$ = MAKE_TERM(ID_IDENT, $1); ")) (alt (terms '-' expression %prec ID_UMINUS) (action_blk " $$ = MAKE_SYMBOL(ID_UMINUS, 1, $2); ")) (alt (terms expression '+' expression) (action_blk " $$ = MAKE_SYMBOL(\'+\', 2, $1, $3); ")) (alt (terms expression '-' expression) (action_blk " $$ = MAKE_SYMBOL(\'-\', 2, $1, $3); ")) (alt (terms expression '*' expression) (action_blk " $$ = MAKE_SYMBOL(\'*\', 2, $1, $3); ")) (alt (terms expression '/' expression) (action_blk " $$ = MAKE_SYMBOL(\'/\', 2, $1, $3); ")) (alt (terms '(' expression ')') (action_blk " $$ = $2; "))))) (code_section ""))
