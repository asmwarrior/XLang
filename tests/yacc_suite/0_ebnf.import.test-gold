<symbol type="grammar">
    <symbol type=":">
        <term type="ident" value=root/>
        <symbol type="|">
            <term type="ident" value=program/>
            <term type="ident" value=error/>
        </symbol>
    </symbol>
    <symbol type=":">
        <term type="ident" value=program/>
        <symbol type="|">
            <term type="ident" value=statement/>
            <symbol type="alt">
                <term type="ident" value=program/>
                <term type="char" value=','/>
                <term type="ident" value=statement/>
            </symbol>
        </symbol>
    </symbol>
    <symbol type=":">
        <term type="ident" value=statement/>
        <symbol type="|">
            <term type="ident" value=expression/>
            <symbol type="alt">
                <term type="ident" value=ID_IDENT/>
                <term type="char" value='='/>
                <term type="ident" value=expression/>
            </symbol>
        </symbol>
    </symbol>
    <symbol type=":">
        <term type="ident" value=expression/>
        <symbol type="|">
            <term type="ident" value=ID_INT/>
            <term type="ident" value=ID_FLOAT/>
            <term type="ident" value=ID_IDENT/>
            <symbol type="alt">
                <term type="char" value='-'/>
                <term type="ident" value=expression/>
                <term type="ident" value=%prec/>
                <term type="ident" value=ID_UMINUS/>
            </symbol>
            <symbol type="alt">
                <term type="ident" value=expression/>
                <term type="char" value='+'/>
                <term type="ident" value=expression/>
            </symbol>
            <symbol type="alt">
                <term type="ident" value=expression/>
                <term type="char" value='-'/>
                <term type="ident" value=expression/>
            </symbol>
            <symbol type="alt">
                <term type="ident" value=expression/>
                <term type="char" value='*'/>
                <term type="ident" value=expression/>
            </symbol>
            <symbol type="alt">
                <term type="ident" value=expression/>
                <term type="char" value='/'/>
                <term type="ident" value=expression/>
            </symbol>
            <symbol type="alt">
                <term type="char" value='('/>
                <term type="ident" value=expression/>
                <term type="char" value=')'/>
            </symbol>
        </symbol>
    </symbol>
</symbol>
