(grammar (rule root (rule_rhs (alt_action program "{ tree_context()->root() = $1; YYACCEPT; }") (alt_action error "{ yyclearin; /* yyerrok; YYABORT; */ }"))) (rule program (rule_rhs (alt_action statement "{ $$ = $1; }") (alt_action (alt program ',' statement) "{ $$ = MAKE_SYMBOL(\',\', 2, $1, $3); }"))) (rule statement (rule_rhs (alt_action expression "{ $$ = $1; }") (alt_action (alt ID_IDENT '=' expression) "{ $$ = MAKE_SYMBOL(\'=\', 2, MAKE_TERM(ID_IDENT, $1), $3); }"))) (rule expression (rule_rhs (alt_action ID_INT "{ $$ = MAKE_TERM(ID_INT, $1); }") (alt_action ID_FLOAT "{ $$ = MAKE_TERM(ID_FLOAT, $1); }") (alt_action ID_IDENT "{ $$ = MAKE_TERM(ID_IDENT, $1); }") (alt_action (alt '-' expression %prec ID_UMINUS) "{ $$ = MAKE_SYMBOL(ID_UMINUS, 1, $2); }") (alt_action (alt expression '+' expression) "{ $$ = MAKE_SYMBOL(\'+\', 2, $1, $3); }") (alt_action (alt expression '-' expression) "{ $$ = MAKE_SYMBOL(\'-\', 2, $1, $3); }") (alt_action (alt expression '*' expression) "{ $$ = MAKE_SYMBOL(\'*\', 2, $1, $3); }") (alt_action (alt expression '/' expression) "{ $$ = MAKE_SYMBOL(\'/\', 2, $1, $3); }") (alt_action (alt '(' expression ')') "{ $$ = $2; }"))))
